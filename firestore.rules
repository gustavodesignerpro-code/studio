rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    // Permite que qualquer pessoa leia qualquer documento no banco de dados.
    // A escrita continua restrita a usu√°rios autenticados.
    match /{document=**} {
      allow read: if true;
      allow write: if request.auth != null;
    }
  }
}
